#include<stdio.h>
int main()

{
    int max=0,num1,num2,i,H;
    scanf("%d",&num1);
    scanf("%d",&num2);
//-------------------------------------------------------------------------------//
//LCM
    
    {
        for(i=num1>num2?num1:num2;i<=num1*num2;i++)
        {
            if(i%num1==0&&i%num2==0)
            {
                
                break;
            }
        }
    }printf("LCM IS: %d",i);
 //--------------------------------------------------------------------------------//
 //HCF
      H=num1<num2?num1:num2;
      for(i=H;i>1;i--)
      {
          if(num1%H==0&&num2%H==0)
          break;
      }
      printf("\nHCF IS:%d",i);
    
}
         

     
