// Harshad Number is an integer that is divisible by the sum of its digits//


#include <stdio.h>

int main()
{
    int num,sum=0,rem,temp,i;
    scanf("%d",&num);
    temp=num;
    while(num!=0)
    {
        rem=num%10;
        sum=sum+rem;
        num=num/10;
    }
    
    if(temp%sum==0)
    printf("yes");
    return 0;
}

